{
  "name": "@layout-css/eslint-plugin-styled-components",
  "version": "0.0.4",
  "description": "An ESLint plugin to validate styled-components do not modify component appearance during composition.",
  "license": "MIT License",
  "author": "Marc Robichaud",
  "main": "build/index.js",
  "scripts": {
    "clean": "rimraf ./build/*",
    "test": "mocha tests/**/*.js --require @babel/register",
    "test:watch": "mocha tests/**/*.js --require @babel/register -w",
    "build": "babel ./src --out-dir ./build",
    "lint": "eslint src/*.js"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "styled-components"
  ],
  "dependencies": {
    "@layout-css/validator": "^0.0.4"
  },
  "peerDependencies": {
    "eslint": ">=0.8.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "devDependencies": {
    "eslint-tester": "^0.8.2"
  }
}
